<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="SettingsListItem">
        <attr name="android:inputType" />
        <attr name="itemTitle" format="string" />
        <attr name="itemHint" format="string" />
        <attr name="editable" format="boolean" />
        <attr name="listType" format="enum">
            <enum name="withEditText" value="0" />
            <enum name="withSpinner" value="1" />
        </attr>

    </declare-styleable>
    <declare-styleable name="EfficiencyChart">
        <attr name="chartColor" format="color"/>
        <attr name="lineColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="WorkoutTimer">
        <attr name="timerColor" format="color"/>
        <attr name="arcColor" format="color"/>
        <attr name="max" format="integer"/>
        <attr name="progress" format="integer"/>
        <attr name="pointerColor" format="color"/>
    </declare-styleable>
</resources>